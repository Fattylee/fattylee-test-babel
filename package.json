{
  "name": "fattylee-storybook",
  "version": "1.0.0",
  "description": "this app is used for managing stories of all kinds",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
   "scripts": {
    "test": "jest --watchAll --verbose",
    "start": "nodemon -w ./public --exec babel-node public --presets=@babel/env",
    "bab": "babel ./api.test.js --out-file=./api.res.js --presets=@babel/env",
    "bab:start": "nodemon --exec babel-node api.test.js"
  },
  "dependencies": {
    "mongoose": "5.4.16",
    "express": "4.16.4",
    "method-override": "3.0.0",
    "express-handlebars": "3.1.0",
    "morgan": "1.9.1",
    "bcryptjs": "2.4.3",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "connect-flash": "0.1.1",
    "express-session": "1.16.2",
    "express-async-errors": "3.1.1",
    "dayjs": "1.8.15",
    "joi": "14.3.1",
    "faker": "4.1.0",
    "express-fileupload": "1.1.5",
    "uuid": "3.3.1",
    "fs-extra": "7.0.1"
  },
  "devDependencies": {
    "jest": "24.1.0",
    "nodemon": "1.19.0"
  },
  "engines": {
    "node": "12.1.0"
